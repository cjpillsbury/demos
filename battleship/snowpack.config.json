{
  "mount": {
    "public": "/",
    "src": "/dist"
  },
  "install": [
    "foldable-device-configurator"
  ],
  "rollup.dedupe": [
    "lit-element",
    "lit-html",
    "spanning-css-polyfill"
  ],
  "plugins": [
    [
      "@snowpack/plugin-babel",
      {
        "input": [".js", ".mjs", ".jsx", ".ts", ".tsx"]
      }
    ]
  ]
}