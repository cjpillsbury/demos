{
  "name": "battleship",
  "version": "1.0.0",
  "description": "A battleship game leveraging dual screen devices.",
  "main": "src/main-application.js",
  "snowpack": {
    "webDependencies": [
      "foldable-device-configurator",
      "lit-element",
      "lit-html",
      "lit-html/directives/class-map.js",
      "spanning-css-polyfill"
    ],
    "dedupe": [
      "lit-element",
      "lit-html",
      "spanning-css-polyfill"
    ]
  },
  "scripts": {
    "start": "es-dev-server --node-resolve --watch --open index.html --port 8081",
    "build": "rimraf web_modules && workbox generateSW workbox-config.js && snowpack --optimize",
    "regen-package-lock": "rimraf node_modules && rimraf package-lock.json; npm install"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "foldable-device-configurator": "^1.0.10",
    "spanning-css-polyfill": "^1.0.12"
  },
  "devDependencies": {
    "es-dev-server": "^1.44.0",
    "eslint": "^6.8.0",
    "eslint-plugin-lit": "1.2.0",
    "babel-eslint": "10.1.0",
    "rimraf": "^3.0.2",
    "snowpack": "^1.6.0",
    "workbox-cli": "^5.1.2"
  },
  "keywords": [],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alexis Menard",
    "email": "alexis.menard@intel.com"
  }
}
