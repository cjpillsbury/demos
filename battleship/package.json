{
  "name": "battleship",
  "version": "1.0.0",
  "description": "A battleship game leveraging dual screen devices.",
  "main": "src/main-application.js",
  "scripts": {
    "start": "concurrently --kill-others \"babel src/ --watch --out-dir dist/\" \"es-dev-server --node-resolve --watch --babel --open index.html --port 8081\"",
    "build": "rimraf build && snowpack build && babel src/ --out-dir dist/ && workbox generateSW workbox-config.js",
    "regen-package-lock": "rimraf package-lock.json && rimraf node_modules && npm install"
  },
  "dependencies": {
    "foldable-device-configurator": "1.1.6",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "nan": "^2.14.1",
    "spanning-css-polyfill": "^1.0.13",
    "workbox-window": "^5.1.3",
    "@material/mwc-button": "^0.17.2",
    "@material/mwc-icon-button": "^0.17.2",
    "@material/mwc-snackbar": "^0.17.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "babel-eslint": "10.1.0",
    "concurrently": "^5.2.0",
    "es-dev-server": "^1.56.1",
    "eslint": "^7.5.0",
    "eslint-plugin-lit": "1.2.0",
    "rimraf": "^3.0.2",
    "snowpack": "^2.6.4",
    "@snowpack/plugin-babel": "^1.4.0",
    "workbox-cli": "^5.1.3"
  },
  "keywords": [],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alexis Menard",
    "email": "alexis.menard@intel.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foldable-devices/demos.git"
  }
}
