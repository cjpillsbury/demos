{
  "name": "photo-gallery",
  "version": "1.0.0",
  "description": "A simple photo gallery that shows how you can support foldable devices.",
  "main": "demo.js",
  "module": "demo.js",
  "snowpack": {
    "webDependencies": [
      "foldable-device-configurator",
      "lit-element",
      "lit-html",
      "lit-html/directives/class-map.js",
      "@material/mwc-checkbox",
      "@material/mwc-drawer",
      "@material/mwc-icon-button",
      "spanning-css-polyfill"
    ],
    "dedupe": [
      "lit-element",
      "lit-html",
      "spanning-css-polyfill"
    ]
  },
  "scripts": {
    "start": "es-dev-server --node-resolve --watch --open index.html --port 8081",
    "build": "rimraf web_modules && snowpack --optimize",
    "regen-package-lock": "rimraf node_modules && rimraf package-lock.json; npm install"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "foldable-device-configurator": "^1.0.5",
    "spanning-css-polyfill": "^1.0.3"
  },
  "devDependencies": {
    "es-dev-server": "^1.44.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@material/mwc-checkbox": "^0.13.0",
    "@material/mwc-drawer": "^0.13.0",
    "@material/mwc-icon-button": "^0.13.0",
    "rollup": "^1.32.1",
    "snowpack": "^1.6.0",
    "rimraf": "^3.0.2"
  },
  "keywords": [],
  "type": "module",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alexis Menard",
    "email": "alexis.menard@intel.com"
  }
}
