{
  "name": "photo-gallery",
  "version": "1.0.0",
  "description": "A simple photo gallery that shows how you can support foldable devices.",
  "main": "demo.js",
  "snowpack": {
    "webDependencies": [
      "foldable-device-configurator",
      "lit-element",
      "lit-html",
      "lit-html/directives/class-map.js",
      "@material/mwc-checkbox",
      "@material/mwc-drawer",
      "@material/mwc-icon-button",
      "spanning-css-polyfill"
    ],
    "dedupe": [
      "lit-element",
      "lit-html",
      "spanning-css-polyfill"
    ]
  },
  "scripts": {
    "start": "concurrently --kill-others \"babel src/ --out-dir dist/ --watch\" \"es-dev-server --node-resolve --watch --babel --open index.html --port 8081\"",
    "build": "rimraf web_modules && snowpack --optimize && babel src/ --out-dir dist/ && workbox generateSW workbox-config.js",
    "regen-package-lock": "rimraf node_modules && rimraf package-lock.json; npm install"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "foldable-device-configurator": "^1.0.12",
    "spanning-css-polyfill": "^1.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@material/mwc-checkbox": "^0.13.0",
    "@material/mwc-drawer": "^0.13.0",
    "@material/mwc-icon-button": "^0.13.0",
    "concurrently": "^5.1.0",
    "es-dev-server": "^1.44.0",
    "rimraf": "^3.0.2",
    "snowpack": "^1.6.0",
    "workbox-cli": "^5.1.2"
  },
  "keywords": [],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alexis Menard",
    "email": "alexis.menard@intel.com"
  }
}
